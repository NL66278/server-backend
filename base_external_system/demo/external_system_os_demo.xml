<odoo>
    <record id="external_system_os" model="external.system">
        <field name="name">Example OS Connection</field>
        <field name="system_type">external.system.os</field>
        <field name="remote_path">/tmp</field>
        <field name="company_ids" eval="[(5, 0), (4, ref('base.main_company'))]" />
    </record>
    <record id="external_system_sink_path" model="external.system.path">
        <field name="system_id" ref="external_system_os" />
        <field name="name">Example external system path where we can put files</field>
        <field name="remote_path">/tmp/sink_path</field>
    </record>
    <record id="external_system_source_path" model="external.system.path">
        <field name="system_id" ref="external_system_os" />
        <field
            name="name"
        >Example external system path from where we can get files</field>
        <field name="remote_path">/tmp/source_path</field>
    </record>
</odoo>
